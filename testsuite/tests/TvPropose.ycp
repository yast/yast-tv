{

    include "testsuite.ycp";

    map READ = $[
	"etc" : $[ "install_inf" : $[] ],
	"target" : $[ "ycp" : $[] ],
	"probe" : $[ "system" : []],
    ];

    TESTSUITE_INIT([READ], nil);
    import "Tv";

	// Tv::cards == nil
	Tv::cards = nil;
	Tv::detected_cards = [ $["module":"bttv", "name":"Second detected", "unique_key":"123xyz"]];
	TEST(``(Tv::Propose()), [], []);
	DUMP(Tv::cards);

	// Tv::cards == card1
	Tv::cards =
	    [ $[ "module" : "bttv",
		 "parameters" : $[ "param1" : "testvalue" ],
		 "unique_key" : "hluk.HvsIesmXXX1",
		 "sound_card_no" : 0
	      ]
	    ];
	Tv::detected_cards = []; // detected_cards cannot have nil value
	TEST(``(Tv::Propose()), [], []);
	DUMP(Tv::cards);

	// Tv::cards == card1, nil, nil, card4
	Tv::cards =
	    [ $[ "module" : "bttv",
		 "parameters" : $[ "param1" : "testvalue" ],
		 "unique_key" : "hluk.HvsIesmXXX1",
		 "sound_card_no" : 0
	      ],
	      nil,
	      nil,
	      $[ "module" : "xyz",
		 "parameters" : $[ "par_other" : "testvalue2" ],
		 "unique_key" : "j7F3.J8dhgs34fK2",
		 "sound_card_no" : 1
	      ]
	    ];
	Tv::detected_cards = [ $["module":"zoran", "name":"First detected", "unique_key":"klic"],
						   $["module":"bttv", "name":"Second detected", "unique_key":"123xyz"]];
	TEST(``(Tv::Propose()), [], []);
	DUMP(Tv::cards);
}
